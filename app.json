{
  "expo": {
    "name": "Hangszercsere",
    "slug": "mobilapp-demo2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hangszercsere",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.snailware.hangszercsere",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#5D1991",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.snailware.hangszercsere"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f714f5a9-c0f0-43f1-931e-d7178a65980c"
      }
    },
    
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5D1991"
    }
  }
}
